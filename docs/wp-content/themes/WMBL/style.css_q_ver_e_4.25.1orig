/*DESKTOP TIMELINE*/

@media all and (min-width: 981px) {

/*Spacing above first item*/

.timeline-section .et_pb_row:first-child .et_pb_column {
  padding-top: 90px;
}

  /*Even rows*/
  .timeline-section .et_pb_row:nth-child(even) {
      display: -webkit-box;
      display: -moz-box;
      display: -ms-flexbox;
      display: -webkit-flex;
      display: flex;
    -webkit-flex-wrap: wrap; /* Safari 6.1+ */
    flex-wrap: wrap;
  }

  .timeline-section .et_pb_row:nth-child(even) .et_pb_column:last-child {
    -webkit-order: 1;
    order: 1;
    padding-right: 5%
  }

  .timeline-section .et_pb_row:nth-child(even) .et_pb_column:first-child {
    -webkit-order: 2;

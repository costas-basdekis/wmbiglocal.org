@import url("fonts.css");

/*GENERAL*/

html, body {
    max-width: 100%;
    overflow-x: hidden;
    font-family: BrownStd;
}

/*Vertical align*/

.vertical-align { 
    display: flex;
    flex-direction: column; 
    justify-content: center;
} 

/*Column mobile stacking order*/

@media all and (max-width: 980px) {
/*** wrap row in a flex box ***/
  .custom-row {
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -webkit-flex;
    display: flex;
  -webkit-flex-wrap: wrap; /* Safari 6.1+ */
  flex-wrap: wrap;
  }
   
  .first-on-mobile {
  -webkit-order: 1;
  order: 1;
  margin-bottom: 30px !important;
  }
   
  .second-on-mobile {
  -webkit-order: 2;
  order: 2;
  }
   
  .third-on-mobile {
  -webkit-order: 3;
  order: 3;
  }
   
  .fourth-on-mobile {
  -webkit-order: 4;
  order: 4;
  }
  /*** add margin to last column
  .custom-row:last-child .et_pb_column:last-child {
  margin-bottom: 30px;
  }***/
}

/*Corner clip*/

.corner-clip {
clip-path: polygon(0% 0%, 0% 100%, calc(100% - 50px) 100%, 100% calc(100% - 50px), 100% 0%);
}

/*Gallery colours*/

.wmbl-gallery .et-pb-slider-arrows {
  color: #e4a93d;
}

.wmbl-gallery .et-pb-slider-arrows a:before, .wmbl-gallery .et-pb-slider-arrows a:after {
  font-size: 48px;
  font-weight: 800;
  text-shadow: 0px 3px 4px #00000050;
}


.wmbl-gallery .et-pb-controllers a {
  background-color: #e4a93d;
  opacity: 0.5;
}

.wmbl-gallery .et-pb-controllers .et-pb-active-control {
  background-color: #e4a93d;
  opacity: 1;
}

/*TYPOGRAPHY*/

h1, h2, h3, h5 {
  font-weight: bold;
}

body {
  font-weight: 300;
  font-style: normal;
}

.intro-text {
  font-size: 26px;

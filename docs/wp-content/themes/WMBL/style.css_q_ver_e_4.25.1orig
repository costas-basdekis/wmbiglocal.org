    top: 4px;
    color: #70baaa;
    font-size: 21px;
}

/*Tick list white*/

.tick-list-white ul {
 list-style-type: none;
 padding-left: 32px;
}

.tick-list-white ul li {
  text-indent: 12px;
}

.tick-list-white ul li:before {
    font-family: 'ETmodules';
    content: "\e052";
    margin: 0px 12px 0 -45px;
    position: relative;
    top: 4px;
    color: #ffffff;
    font-size: 21px;
}

/*Tick list red*/

.tick-list-red ul {
 list-style-type: none;
 padding-left: 32px;
}

.tick-list-red ul li {
  text-indent: 12px;
}

.tick-list-red ul li:before {
    font-family: 'ETmodules';
